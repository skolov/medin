extends ../layout/layout

block variables
  - var title = 'UI-KIT';

block header
  +b.HEADER.UIheader
    +e.top
      +e.trigger
        +e.trigger-link(href="#")
          //+e.IMG(src='assets/img/UIright.svg')
      +e.top-left
        +b.UIlogo
          +e.link(href='/')
            +e.IMG(src='data:image/png;base64,').pic

      +e.top-right
        +b.UInav
          +e.item
            +e.item-link(href="/design.html") WEB-принципы
          +e.item.active
            +e.item-link(href="/index.html") Сайты

        //- +b.UIform-search
        //-   form
        //-     +e.wrap
        //-       +e.input
        //-         input(type='text')
        //-       +e.btn
        //-         button(type='submit').UIbtn.btn-icon
        //-          +e.IMG(src='assets/img/UIsearch-icon.png').btn-pic

      +e.burger
        +b.UIburger-menu(href="#")
          +e.icon
    +e.bottom(style= "background-color: #1258A6")
      +b.UItitle-line
        +e.subtitle # 468
        +e.main
          h2 ГлавУпДК



block content
  +b.UIpage
    +e.sidebar
      +b.UIsidebar
        +e.panel.e-scroll
          +b.UIlist
            +e.item.active
              +e.item-link(href='javascript:void(0);') Компоненты
            //- +e.item
            //-   +e.item-link(href='#') Страницы

        +e.slide.e-scroll
          +b.UIlist.--state
            +e.search
              +b.UIinput-filter
                form
                  input(type='text' placeholder='Поиск')
            +e.items
              -
                const components = [
                ]

              each component, index in components
                +e.item(class= index == 0 ? "active" : "")
                  +e.item-link(href=`${component}.html`)= component

    +e.content
