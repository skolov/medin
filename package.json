    "popper.js": "^1.14.3",
    "priority-nav": "^1.0.13",
    "rangy": "^1.3.0",
    "readmore-js": "^2.1.0",
    "slick-carousel": "^1.8.1",
    "slideout": "^1.0.1",
    "sticky-sidebar": "^3.3.1",
    "svg-pan-zoom": "^3.6.0",
    "tabulator-tables": "^4.0.5",
    "texttailor.js": "https://github.com/jpntex/TextTailor.js/",
    "tingle.js": "^0.15.1",
