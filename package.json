    "lint-staged": "^9.2.5",
    "mini-css-extract-plugin": "0.4.0",
    "node-sass": "4.12.0",
    "node-sass-glob-importer": "^5.3.0",
    "nodemon": "^1.17.4",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-loader": "^2.1.5",
    "postcss-pxtorem": "^4.0.1",
    "prettier": "^1.18.2",
    "pug-html-loader": "^1.1.5",
