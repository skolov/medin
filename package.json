    "vide": "^0.5.1",
    "vue": "^2.6.6",
    "vue-router": "^3.0.2",
    "vuex": "^3.1.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.(css|scss|js|vue)": [
      "yarn code-format",
      "git add"
    ],
    "*.(js|vue)": [
      "yarn lint:js",
      "git add"
    ],
    "*.(css|scss|vue)": [
      "yarn lint:style",
      "git add"
    ]
  }
}
