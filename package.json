    "bootstrap": "^4.1.1",
    "bulma": "^0.7.4",
    "cleave.js": "^1.5.0",
    "devbridge-autocomplete": "https://github.com/devbridge/jQuery-Autocomplete",
    "dropzone": "^5.4.0",
    "family.scss": "^1.0.8",
    "fancybox": "2.1.7",
    "flag-icon-css": "^3.0.0",
    "fontfaceobserver": "^2.0.13",
    "frappe-charts": "^1.1.0",
    "highlight.js": "^9.12.0",
    "inputmask": "^4.0.0",
    "jcf": "^1.2.3",
    "jquery": "^3.3.1",
    "jquery-gray": "^1.6.0",
    "jquery.countdown": "^1.2.8",
    "lodash-es": "^4.17.15",
    "malihu-custom-scrollbar-plugin": "^3.1.5",
    "masonry-layout": "^4.2.1",
    "mediaelement": "^4.2.9",
    "most-visible": "^1.2.1",
    "mousetrap": "^1.6.2",
