{
  "name": "KSS",
  "version": "2.0.0",
  "description": "Шаблон на основе webpack, pug, scss, es6, postcss для многостраничных приложений",
  "main": "./app/js/index.js",
  "license": "MIT",
  "author": "Компания Сайтсофт",
  "scripts": {
    "analyze": "webpack-bundle-analyzer ./stats.json",
    "build": "webpack --env production --progress --buildStatus project",
    "start": "nodemon --watch webpack.config.js --buildStatus project --exec \"webpack-dev-server --open --hot --env development\"",
    "build-ui": "webpack --env production --progress --buildStatus ui-kit",
    "start-ui": "nodemon --watch webpack.config.js --buildStatus ui-kit --exec \"webpack-dev-server --open --env development\"",
    "code-format": "prettier --write app/**/*.{css,scss,js,vue}",
    "lint:js": "eslint --ext .js --ext .vue ./app/ --fix",
    "lint:js-debug": "eslint --ext .js --ext .vue ./app/",
    "lint:style": "stylelint  ./app/**/*.css ./app/**/*.scss ./app/**/*.vue --fix",
    "lint:style-debug": "stylelint ./app/**/*.css, ./app/**/*.scss, ./app/**/*.vue"
  },
  "keywords": [
    "boilerplate",
    "webpack",
    "pug",
    "scss",
    "es6",
    "postcss",
    "vue"
  ],
  "devDependencies": {
    "@babel/cli": "7.1.2",
    "@babel/core": "7.1.2",
    "@babel/plugin-syntax-dynamic-import": "7.0.0",
    "@babel/preset-env": "7.1.0",
    "@vue/eslint-config-prettier": "^5.0.0",
    "@vue/eslint-config-standard": "^4.0.0",
    "autoprefixer": "^9.1.5",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^8.0.4",
